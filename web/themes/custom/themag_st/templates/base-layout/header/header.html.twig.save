{% block top_bar %}
  {% if page.top_bar_first or page.top_bar_second %}
    <div class="top-bar">
      <div class="container top-bar__container">
        <div class="row justify-content-between align-items-center">
          <div class="col-auto top-bar__left">{{ page.top_bar_first }}</div>
          <div class="col-auto top-bar__right">{{ page.top_bar_second }}</div>
        </div>
      </div>
    </div>
  {% endif %}
{% endblock %}


