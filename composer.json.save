    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "cweagans/composer-patches": true,
            "oomphinc/composer-installers-extender": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/dropzonejs/composer.libraries.json"
            ]
        },
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        },
	"patches": {
            "drupal/core": {
                "3062742: Drag-and-drop reordering doesn't work if the region content is nested in other elements": "https://www.drupal.org/files/issues/2021-02-01/3062742_10_0.patch",
                "Undefined options and Count Warning in Radios.php": "https://www.drupal.org/files/issues/2022-07-20/3298714.patch",
                "Allow views attachment display use own pager options": "https://www.drupal.org/files/issues/2020-11-29/2833734-allow-attachment-pager-42.patch",
                "Incorrect order and duplicate theme hook suggestions1": "https://www.drupal.org/files/issues/2020-11-23/2118743-190.patch",
                "Incorrect order and duplicate theme hook suggestions": "https://www.drupal.org/files/issues/2022-04-19/2752443-73.patch",
		"Enclosure in RSS": "https://www.drupal.org/files/issues/2023-10-16/core-support-enclosure-field-rss-2511878-64.patch",
		"confusing errorand behavior when creating a new referenced": "https://www.drupal.org/files/issues/2021-12-27/2924783-34.patch"
            },
            "drupal/layout_library": {
                "3133484: Form blocks rendered inside layout builder library break save": "https://www.drupal.org/files/issues/2020-05-04/layout-library-save-issue-3133484-4.patch"
            },
            "drupal/entity_browser": {
                "2858438: Provide inline entity form FieldWidgetDisplay": "https://www.drupal.org/files/issues/2019-07-04/2858438_17.patch"
            },
            "drupal/poll": {
                "2331515: Allow to show the poll question in the vote/result templates": "https://www.drupal.org/files/issues/2019-03-15/question-title-is-displayed-as-block-title-2923946-8.x-1.x-dev-7.patch"
            },
            "drupal/better_exposed_filters": {
                "3243845: TypeError after D9 update": "https://www.drupal.org/files/issues/2021-10-18/3243845-5.patch"
            },
            "drupal/we_megamenu": {
                "3239043: Deprecated error /PHP. 8.0 Drupal 9": "https://www.drupal.org/files/issues/2022-02-24/we_megamenu_deprecated.patch",
                "3270040: Fix Error when trying to add new menu link as sub-link of the menu item": "https://www.drupal.org/files/issues/2022-03-30/3270040-6.patch",
                "3226361: TypeError: Cannot access offset of type string on string" : "https://www.drupal.org/files/issues/2022-05-30/3226361_10.patch"
            },
            "drupal/social_link_field": {
                "3230579:update hardcoded youtube url _7": "https://www.drupal.org/files/issues/2023-02-14/3230579_update-hardcoded-youtube-url_7.patch"
            },
            "drupal/pdf_preview_image": {
                "2953786: There were errors validating the config synchronization": "https://www.drupal.org/files/issues/2018-03-16/fix_module_name-2953786-3.patch"
            },
            "drupal/body_inject": {
                "Agg-net specific - correction of paragraph explode": "patches/aggnet_paragraphs_fix.patch"
            },
            "drupal/poll": {
	       "Sometimes not possible to vote The specified #ajax callback is empty or not callable": "https://www.drupal.org/files/issues/2023-12-06/poll-3263876-19.patch"
	    }		
        },
        "enable-patching": "true",
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
